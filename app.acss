/*通用提交按钮*/
.submit-btn{
  color: #fff;
  background: #1e6ce8;
  border-color: #1e6ce8;
  height: 30px;
  height: var(--button-small-height,30px);
  line-height: 30px;
  line-height: var(--button-small-line-height,30px);
  padding: 0 8px;
  padding: 0 var(--padding-xs,8px);
  font-size: 12px;
  font-size: var(--button-small-font-size,12px);
  border-radius: 999px;
  border-radius: var(--button-round-border-radius,999px)
}
.submit-btn.large{
  background: #1e6ce8;
  border-color: #1e6ce8;
  height: 38px;
  height: var(--button-small-height,38px);
  line-height: 38px;
  line-height: var(--button-small-line-height,38px);
  padding: 0 8px;
  padding: 0 var(--padding-xs,8px);
  font-size: 14px;
  font-size: var(--button-small-font-size,14px);
  border-radius: 999px;
  border-radius: var(--button-round-border-radius,999px)
}
.submit-btn.plain{
  background: #fff;
  border-color: #1e6ce8;
  border:1px solid #1e6ce8;
  color: #1e6ce8;
 
}
.submit-btn.JQBtn{
  background: #F04134;
  border-color: #F04134;
  width: 240rpx;
  margin: 0 auto;
  color: #fff;
  text-align: center
}
.submit-btn.delete{
  background: #666;
  border-color: #666;
  color: #fff;
  text-align: center
}
/*表格*/
.table {
  border: 1px solid #eee;
  border-right: 0;
  border-bottom: 0;
  width: 100%;
  margin-top: 20px;
}

.tr {
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.th, .td {
  height: 64rpx;
  line-height: 64rpx;
  border-bottom: 1px solid #eee;
  border-right: 1px solid #eee;
  text-align: center;
  width: 100%;
  font-size:20rpx;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}
.th {
  font-weight: 400;
  background-color: #f9f9f9;
}
.th1{
  width:20%;
}
