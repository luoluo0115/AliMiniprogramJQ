/*发票列表公用样式 左图片*/

.expend-list {
  width: 100%;
  margin-bottom: 20rpx;
}

.expend-list .expend-item {
  background-color: #fff;
  margin-bottom: 20rpx;
  overflow: hidden;
}


.expend-list .list-title {
  font-size: 28rpx;
  color: #333;
  padding: 30rpx 0 25rpx 30rpx;
  border-bottom: 1px solid #eee;
}
.expend-list .title {

  height: 64rpx;
  line-height: 64rpx;
  font-size: 22rpx;
  color: #666;
  border-bottom: 1px solid #eee;
  padding: 0 30rpx;
}

.expend-list .a-expend {
  display: flex;
  padding:15rpx 30rpx;
  border-bottom: 1px solid #eee;
  font-size: 20rpx;

}

.expend-list .a-expend .img-box {
  width: 180rpx;
  height: 180rpx;
  overflow: hidden;
  margin-right: 20rpx;
  border-radius: 5rpx;
}

.expend-list .img-box .img {
  width: 100%;
  height: 100%;
}

.expend-list .a-expend .text-box {
  width: 100%;
  box-sizing: border-box;
  padding-top: 10rpx;
}

.expend-list .a-expend .text-box .arow {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.expend-list .a-expend .text-box .arow .label {
  width: 330rpx;
  font-size: 20rpx;
  height: 40rpx;
  line-height: 1.6;
  overflow: hidden;
}
 

.expend-list .a-expend .text-box .arow .label-right {
  font-size: 20rpx;
  align-self: flex-start;

}

.expend-list .pay-btn {
  border-radius: 20rpx;
  height: 38rpx;
  line-height: 38rpx;
  font-size: 20rpx;
  color: #F0433D;
  border: 1px solid #F0433D;
  text-align: center;
  padding: 0 20rpx;
  margin: 12rpx 0 5rpx 20rpx;
  display: inline-block;
  float: right;
}

.expend-list .pay-btn.delete {
  color: #999;
  border: 1px solid #999;
}

 

.expend-list .btn-list{
  margin: 20rpx 0 0;
  text-align: right;
}
