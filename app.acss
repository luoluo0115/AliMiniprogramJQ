/*公共tag标签*/
.van-tag {
    display: -webkit-inline-flex;
    display: inline-flex;
    -webkit-align-items: center;
    align-items: center;
    line-height: normal;
    padding: .2em .5em;
    padding: var(--tag-padding,.2em .5em);
    color: #fff;
    color: var(--tag-text-color,#fff);
    font-size: 10px;
    font-size: var(--tag-font-size,10px);
    border-radius: .2em;
    border-radius: var(--tag-border-radius,.2em);
}
.van-tag.primary {
    background-color: #1989fa;
    background-color: var(--tag-primary-color,#1989fa);
}
.van-tag.success {
    background-color: #07c160;
    background-color: var(--tag-success-color,#07c160)
}
.van-tag.van-tag-plain {
    background-color: #fff;
    background-color: var(--tag-plain-background-color,#fff);
}
.van-tag.van-tag-plain {
    background-color: #fff;
    background-color: var(--tag-plain-background-color,#fff);
}
.van-tag.plain-1e6ce8 {
    background-color: #fff;
    background-color: var(--tag-plain-background-color,#fff);
    border:1px solid #1e6ce8;
    color: #1e6ce8;
    border-radius: 5rpx
}
.van-tag.plain-7966FE{
    background-color: #fff;
    background-color: var(--tag-plain-background-color,#fff);
    border:1px solid #7966FE;
    color: #7966FE;
    border-radius: 5rpx
}
.van-tag.round{
